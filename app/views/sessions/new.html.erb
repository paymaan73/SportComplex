<h1>Login path</h1>

<%= form_for(:session, url: login_url) do |f| %>
    <div>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </div>

    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
<div>
    <%= f.submit "Login" %>
</div>
<% end %>
